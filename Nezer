import telebot
from telebot import types

bot = telebot.TeleBot('TOKEN')

@bot.message_handler(commands=['start'])
def start(message):
    markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
    btn1 = telebot.types.KeyboardButton("Выбрать персонажа")
    markup.add(btn1)
    bot.send_message(message.chat.id, "Здравствуйте!Выберите любого вам интересующего персонажа", reply_markup=markup)

@bot.message_handler(func=lambda message: message.text == "Выбрать персонажа")
def choose_character(message):
    markup = types.ReplyKeyboardMarkup(resize_keyboard=True, row_width=3)
    character = [
        "Лорд Кралос", "Король Нэус", "Король Тускарр",
        "Король Дума", "Король Зорн", "Король Бельфагор",
        "Король Ворация", "Король Глютон", "Король Поркшин",
        "Король Гамлет", "Ургракк", "Аврора Голдфист",
        "Эбигейл Кристеффор", "Дурркол Стяжатель", "Нуриэль",
        "Рэйф", "Солемнус", "Инферниус",
        "Белла Кристеффор", "Ванесса Кристеффор", "Баатор Базальтоход",
        "Вуртскарг", "Командир Бортс", "Ложный король свинозомби",
        "Маммон", "Лорд Зарксон", "Кайя",
        "Хогсворд", "Акуи Азурлумен"
    ]
    buttons = [types.KeyboardButton(char) for char in character]
    markup.add(*buttons)
    bot.send_message(message.chat.id, "Выберите вам интересуещего персонажа", reply_markup=markup)

@bot.message_handler(func=lambda message: message.text in [
    "Лорд Кралос", "Король Нэус", "Король Тускарр",
        "Король Дума", "Король Зорн", "Король Бельфагор",
        "Король Ворация", "Король Глютон", "Король Поркшин",
        "Король Гамлет", "Ургракк", "Аврора Голдфист",
        "Эбигейл Кристеффор", "Дурркол Стяжатель", "Нуриэль",
        "Рэйф", "Солемнус", "Инферниус",
        "Белла Кристеффор", "Ванесса Кристеффор", "Баатор Базальтоход",
        "Вуртскарг", "Командир Бортс", "Ложный король свинозомби",
        "Маммон", "Лорд Зарксон", "Кайя",
        "Хогсворд", "Акуи Азурлумен"
])

@bot.message_handler(func=lambda message: message.text in character_info)
def show_character_info(message):
    character_name = message.text
    info = character_info[character_name]
    respons = f"{character_name}\n\n{info['description']}"
    if 'photo' in info:
        bot.send_photo(message.chat.id, info['photo'], caption=respons)
    else:
        bot.send_message(message.chat.id, respons)

